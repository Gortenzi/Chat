ntent.querySelector('.message-time__other').textContent = getTime(DATA.TIME_MESSAGE);\r\n  let elem = templ.content.cloneNode(true);\r\n  \r\n\r\n    chat.append(elem);\r\n    \r\n\r\n }\r\n\r\n}\r\n\r\n\r\n export const getTime = () => {\r\n\r\n    const date = new Date();\r\n    const hours = date.getHours();\r\n    let minutes = date.getMinutes();\r\n    minutes = (minutes<10) ? '0'+minutes: minutes;\r\n    return `${hours}:${minutes}`;\r\n  }\r\n\r\n"},"lineCount":null}},"error":null,"hash":"3813aa239b6e5accac349c324a95c17c","cacheData":{"env":{}}}